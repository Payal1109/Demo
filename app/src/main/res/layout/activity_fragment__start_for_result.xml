<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragments_StartActivityForResultPackage.Fragment_StartActivityForResult"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar"
        android:background="@color/colorPrimary"
        app:title="Fragments_StartActivityForResult">

    </androidx.appcompat.widget.Toolbar>

    <Button
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:id="@+id/fragtoactivity"
        android:layout_gravity="center"
        android:text="FragmentToActivity" />
    <Button
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:id="@+id/mulfrag"
        android:layout_gravity="center"
        android:text="MultipleFragments"/>
    <Button
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:id="@+id/forresult"
        android:layout_gravity="center"
        android:text="StartActivityForResult"/>

</LinearLayout>