<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activity_IntentPackage.ImplicitIntent.ImplicitIntent">

    <androidx.appcompat.widget.Toolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/toolbar"
        android:background="@color/colorPrimary"
        app:title="Implicit_Intent">

    </androidx.appcompat.widget.Toolbar>

    <Button
        android:id="@+id/LaunchMap"
        android:layout_width="265dp"
        android:layout_height="60dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="73dp"
        android:layout_marginLeft="126dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="73dp"
        android:layout_marginRight="73dp"
        android:layout_marginBottom="10dp"
        android:background="@color/colorAccent"
        android:onClick="process"
        android:text="Launch Map"
        android:textSize="25dp" />

    <Button
        android:id="@+id/LaunchMarket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/LaunchMap"
        android:layout_alignRight="@+id/LaunchMap"
        android:layout_below="@+id/LaunchMap"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="process"
        android:background="@color/colorAccent"
        android:text="Launch Market"
        android:textSize="25dp"/>

    <Button
        android:id="@+id/sendEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/LaunchMarket"
        android:layout_alignRight="@+id/LaunchMarket"
        android:layout_below="@+id/LaunchMarket"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="process"
        android:background="@color/colorAccent"
        android:text="Send Email"
        android:textSize="25dp"/>

    <Button
        android:id="@+id/sendImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sendEmail"
        android:layout_alignRight="@+id/sendEmail"
        android:layout_below="@+id/sendEmail"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="process"
        android:background="@color/colorAccent"
        android:text="Send Image"
        android:textSize="25dp"/>

    <Button
        android:id="@+id/contacts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/sendImage"
        android:layout_alignRight="@+id/sendImage"
        android:layout_below="@+id/sendImage"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="process"
        android:background="@color/colorAccent"
        android:text="Contacts"
        android:textSize="25dp"/>

    <Button
        android:id="@+id/dialCall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contacts"
        android:layout_alignRight="@+id/contacts"
        android:layout_below="@+id/contacts"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:onClick="process"
        android:background="@color/colorAccent"
        android:text="Telephone"
        android:textSize="25dp"/>




</RelativeLayout>